package Tema1;

import java.util.Scanner;

public class Ex6 {

	public static void main(String[] args) {
	
		Scanner input = new Scanner(System.in);
		        
		System.out.print("Introdu un numar: ");
		int num = input.nextInt();
		        
		if (num < 0) {
		    num = -num;
		   }

		while (num > 0) {
		    int cifra = num % 10;
		    System.out.println(cifra);
		      num = num / 10;
		}

		 input.close();
		 
		}
	}
