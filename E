package Tema1;

public class Ex8  {

    public static void main(String[] args) {

        int number = 4;
        long fact = 1;
        int i = 1;
        while(i<=number)
        {
            fact = fact * i;
            i++;
        }
        System.out.println("Factorul numÄƒrului "+number+" este: "+fact);
    }
}
