package Tema1;

import java.util.Scanner;

public class Ex17 {

	public static void main(String[] args) {
		
	Scanner scanner=new Scanner(System.in);
	System.out.print("Introdu numarul: ");
		int n=scanner.nextInt();
		int max=0,cif;
		while(n!=0)
		{
		cif=n%10;
		if(cif>max)
		max=cif;
		n/=10;
		}
	System.out.println(max);
	scanner.close();
	}

}
